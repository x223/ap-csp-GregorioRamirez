function setup() { 
  createCanvas(windowWidth, windowHeight);
} 

function draw() { 
  background(0);
  strokeWeight(4);
  stroke(255); 

  y = 60; 
  for (var x = 25; x < windowWidth; x = x + 60) { 
  	for (var y = 25; y < windowHeight; y = y + 60) {  
    fill ('yellow')
      ellipse(x, y, 60, 60);
  }

	}
}
